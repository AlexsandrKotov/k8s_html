pipeline {
    agent any
    stages {
       stage('Deploy Patient App') {
         steps {
             withCredentials([
                  string(credentialsId: 'my_kubernetes', variable: 'api_token')
                  ]) {
                   sh 'kubectl --token $api_token --server https://192.168.99.101:8443 --insecure-skip-tls-verify=true apply -f k8s_html.yaml'
                     }
               }
           }

    }
}


